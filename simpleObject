public class object {

		//instant variables
		String breed;
		double height;
		int age;
		String color;
		
		//instant method
		public String getInfo() {
			return ("My dog's breed is: "+ breed +"\n"+ "My dog's height is: "+ height +"\n"+
		"My dog's age is: "+ age+"\n"+ "My dog's color is: "+ color);
		}
		
          public static void main(String[] args) {
         		object myDog = new object();
         		object my2ndDog = new object();
         		myDog.breed = "Husky";
         		myDog.height = 2.4;
         		myDog.age = 5;
         		myDog.color = "Black and White";
         		my2ndDog.breed = "Poodle";
         		my2ndDog.height = 1.1;
         		my2ndDog.age = 8;
         		my2ndDog.color = "White";
       
         		prln(myDog.getInfo());
         		prln(my2ndDog.getInfo());

	}

	public static void prln(Object myObject) {
		System.out.println(myObject);
	}
	
}

		
	
